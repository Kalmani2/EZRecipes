{
  "version": 2,
  "rewrites": [
    { "source": "/(.*)", "destination": "/api" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; img-src 'self' data: https://ezrecipes-qhn5rhec6-kalmanis-projects.vercel.app/favicon.ico; ..."
        }
      ]
    }
  ]
}
